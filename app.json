    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "54abd29f-2b87-43be-abcd-eef7966d1693"
      }
    },
    "plugins": ["@config-plugins/react-native-ble-plx"]
